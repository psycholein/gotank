<div class="module <%= @module %>" data-name="<%= @name %>">
  <a class="control" data-event="forward">Forward</a>
  <a class="control" data-event="backward">Backward</a>
  <a class="control" data-event="turnleft">turnleft</a>
  <a class="control" data-event="turnright">turnright</a>
  <a class="control" data-event="stop">stop</a>
</div>
